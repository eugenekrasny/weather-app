{
  "name": "weather-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.15.3",
    "material-design-icons": "^3.0.1",
    "moment": "^2.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "weather-icons": "^1.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
